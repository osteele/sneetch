{
  "compilerOptions": {
    "module": "commonjs",
    "outDir": "./dist/",
    "sourceMap": true,
    "target": "es2017",

    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    // "noImplicitUseStrict": true,
    // "noUnusedLocals": true,
    "strict": true,
    "strictNullChecks": true
  }
}
